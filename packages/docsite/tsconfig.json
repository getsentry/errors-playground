{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/layouts/*": ["src/layouts/*"],
      "@/content/*": ["src/content/*"],
      "@/components/*": ["src/components/*"],
      "@/styles/*": ["src/styles/*"]
    }
  }
}
